class Solution {
public:
    vector<vector<int>> matrixReshape(vector<vector<int>>& mat, int r, int c) {
        int m=size(mat);
        // int i=0;
        int n=size(mat[0]);
        if((m*n)!=(r*c))return mat;
        vector<vector<int>>a(r,vector<int>(c));
        for(int i=0;i<(m*n);i++){
            a[i/c][i%c]=mat[i/n][i%n];
        }
        return a;
    }
};
